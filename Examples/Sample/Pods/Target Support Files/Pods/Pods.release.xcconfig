GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/DZNWebViewController" "${PODS_ROOT}/Headers/Public/MMMarkdown"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DZNWebViewController" -isystem "${PODS_ROOT}/Headers/Public/MMMarkdown"
OTHER_LDFLAGS = -ObjC -l"Pods-DZNWebViewController" -l"Pods-MMMarkdown" -framework "UIKit" -framework "WebKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods